<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
         http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
  <key id="ftuName" for="node" attr.name="ftuName" attr.type="string"/>
  <key id="fileFormat" for="node" attr.name="fileFormat" attr.type="string"/>
  <key id="color" for="node" attr.name="color" attr.type="string"/>
  <key id="size" for="node" attr.name="size" attr.type="double"/>
  <key id="cell_type" for="node" attr.name="cell_type" attr.type="string"/>
  <key id="id" for="edge" attr.name="id" attr.type="string"/>
  <graph id="G" edgedefault="undirected">
    <node id="1">
      <data key="ftuName">ftu1</data>
      <data key="fileFormat">ply</data>
      <data key="color">#ff0000</data>
      <data key="size">1.0</data>
      <data key="cell_type">type1</data>
    </node>
    <node id="2">
      <data key="ftuName">ftu2</data>
      <data key="fileFormat">ply</data>
      <data key="color">#00ff00</data>
      <data key="size">1.0</data>
      <data key="cell_type">type2</data>
    </node>
    <node id="3">
      <data key="ftuName">ftu1</data>
      <data key="fileFormat">ply</data>
      <data key="color">#ff0000</data>
      <data key="size">1.0</data>
      <data key="cell_type">type1</data>
    </node>
    <edge source="1" target="2">
      <data key="id">1-2</data>
    </edge>
    <edge source="2" target="3">
      <data key="id">2-3</data>
    </edge>
    <edge source="1" target="3">
      <data key="id">1-3</data>
    </edge>
  </graph>
</graphml> 